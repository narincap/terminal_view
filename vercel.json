{
  "functions": {
    "api/**/*.py": {
      "memory": 1024,
      "maxDuration": 60
    },
    "api/screener/momentum.py": {
      "memory": 3008,
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/api/stock/:symbol",
      "destination": "/api/stock/[symbol].py"
    },
    {
      "source": "/api/screener/momentum",
      "destination": "/api/screener/momentum.py"
    }
  ]
}
